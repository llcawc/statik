{
  "name": "statik",
  "version": "0.0.3",
  "date": "2022.08.15",
  "license": "ISC",
  "author": "pasmurno by llcawc",
  "homepage": "https://github.com/llcawc",
  "description": "statik & nunjucks test ",
  "keywords": [
    "statik",
    "11ty",
    "nunjucks",
    "starter"
  ],
  "main": "index.js",
  "scripts": {
    "cleandist": "rm -rf dist",
    "11ty": "npx @11ty/eleventy",
    "test": "DEBUG=Eleventy* npx @11ty/eleventy",
    "serve": "browser-sync dist --port 3000 -b 'firefox' --no-notify",
    "dev": "npm-run-all cleandist --parallel dev:*",
    "dev:11ty": "npx @11ty/eleventy --serve --port=3000 --watch",
    "dev:js": "rollup -c -w",
    "dev:css": "postcss src/assets/styles/main.postcss -o src/assets/css/main.min.css -w",
    "build": "npm-run-all cleandist build:* 11ty",
    "build:js": "rollup -c --environment BUILD:production",
    "build:css": "NODE_ENV=production postcss src/assets/styles/main.postcss -o src/assets/css/main.min.css"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^14.1.0",
    "postcss-scss": "^4.0.4",
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "markdown-it": "^13.0.1"
  }
}
